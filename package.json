{
  "name": "media-master-ultimate-plus",
  "version": "1.0.0",
  "description": "MEDIA MASTER - Ultimate Media Organizer Plus",
  "main": "./dist/main/main.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "concurrently \"npm run dev:webpack\" \"npm run dev:electron\"",
    "dev:webpack": "cross-env NODE_ENV=development webpack --watch",
    "dev:electron": "cross-env NODE_ENV=development electron-forge start",
    "build": "cross-env NODE_ENV=production webpack && electron-forge make",
    "build:deb": "cross-env NODE_ENV=production webpack && electron-forge make --targets=@electron-forge/maker-deb",
    "build:win": "cross-env NODE_ENV=production webpack && electron-forge make --targets=@electron-forge/maker-squirrel",
    "build:mac": "cross-env NODE_ENV=production webpack && electron-forge make --targets=@electron-forge/maker-dmg,@electron-forge/maker-zip",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --ext .ts,.tsx . --fix",
    "test": "jest"
  },
  "keywords": [
    "media",
    "organize",
    "electron",
    "photo",
    "video"
  ],
  "author": "Batur Kacamak",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.7.0",
    "@electron-forge/maker-deb": "^7.7.0",
    "@electron-forge/maker-dmg": "^7.7.0",
    "@electron-forge/maker-rpm": "^7.7.0",
    "@electron-forge/maker-squirrel": "^7.7.0",
    "@electron-forge/maker-zip": "^7.7.0",
    "@tailwindcss/postcss": "^4.0.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^34.3.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-loader": "^2.1.0",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.3",
    "sass": "^1.85.1",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.0.9",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.0",
    "axios": "^1.6.7",
    "electron-log": "^5.3.1",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.1",
    "exifreader": "^4.26.1",
    "i18next": "^24.2.2",
    "image-js": "^0.36.0",
    "mini-css-extract-plugin": "^2.9.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.2.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0"
  }
}
